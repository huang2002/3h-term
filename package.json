{
  "name": "term-js",
  "version": "0.1.0",
  "description": "A terminal simulator in browser.",
  "module": "./dist/term-js.min.js",
  "main": "./dist/term-js.umd.min.js",
  "types": "./index.d.ts",
  "author": "3h",
  "license": "MIT",
  "scripts": {
    "prebuild": "tsc",
    "build": "rollup -c",
    "postbuild": "terser dist/term-js.js --config-file terser.json --module -o dist/term-js.min.js && terser dist/term-js.umd.js --config-file terser.json -o dist/term-js.umd.min.js && node stat",
    "docs": "dts2md \"**\" \"!index.d.ts\" -i ./types -o ./docs -l -I Home.md"
  },
  "repository": "huang2002/term-js",
  "keywords": [
    "t",
    "term",
    "terminal",
    "shell",
    "console",
    "simulator",
    "3h"
  ],
  "files": [
    "dist",
    "types",
    "index.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "dts2md": "^0.4.0",
    "rollup": "^2.11.0",
    "rollup-plugin-babel": "^4.3.0",
    "terser": "^5.2.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "super-x": "^1.0.0"
  }
}
